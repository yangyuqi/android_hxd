<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_gray" >
    <com.zhejiang.haoxiadan.ui.view.CommonTitle
        android:id="@+id/common_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titleStr="@string/title_company_file"
        app:rightIcon="@drawable/btn_consulting"
        app:showRightIcon="true"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/common_title"
        android:layout_marginTop="@dimen/top_distance"
        android:background="@color/bg_white"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_company_"/>
                <TextView
                    android:id="@+id/tv_company"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:background="@color/transparent"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_license_number_"/>
                <TextView
                    android:id="@+id/tv_license_number"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    style="@style/text_black_14"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_enterprise_type_"/>
                <TextView
                    android:id="@+id/et_enterprise_type"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    style="@style/text_black_14"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_employees_num_"/>
                <TextView
                    android:id="@+id/et_employees_num"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    style="@style/text_black_14"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_1"
                android:background="@color/line_gray_cc"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_shop_introduction_"/>
                <TextView
                    android:id="@+id/tv_shop_introduction"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    style="@style/text_black_14"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_main_industry_"/>
                <TextView
                    android:id="@+id/tv_main_industry"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_product_style_"/>
                <TextView
                    android:id="@+id/tv_product_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_main_products_"/>
                <TextView
                    android:id="@+id/tv_main_products"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_main_customergroups_"/>
                <TextView
                    android:id="@+id/tv_main_customergroups"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_1"
                android:background="@color/line_gray_cc"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_month_production_"/>
                <TextView
                    android:id="@+id/tv_month_production"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_year_sales_"/>
                <TextView
                    android:id="@+id/tv_year_sales"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/value_add_apply_item_height"
                android:paddingLeft="@dimen/distance"
                android:paddingRight="@dimen/distance"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"
                    android:text="@string/label_year_export_num_"/>
                <TextView
                    android:id="@+id/tv_year_export_num"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    style="@style/text_black_14"
                    android:gravity="center_vertical"/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
